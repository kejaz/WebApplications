@model IEnumerable<MVCRazor.Models.Employee>
@{
    ViewBag.Title = "Index";
}

<h2>Employees</h2>
@Html.ActionLink("Create New","Create")
<br>
<table  class="table" >
    <tr>
        <th>
            Employee ID
        </th>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Gender
        </th>
        <th>
            Salary
        </th>
        <th>
            
        </th>
    </tr>
    @foreach (var emp in Model)
    {
        <tr>
            <td>
                @emp.ID
            </td>
            <td>
               @emp.FirstName
            </td>
            <td>
                @emp.LastName
            </td>
            <td>
                @emp.Gender
            </td>
            <td>
                @emp.Salary
            </td>
            <td>
                @Html.ActionLink("Details","Details",new {id = emp.ID }) |
                @Html.ActionLink("Edit", "Edit", new { id = emp.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = emp.ID }) 
            </td>
        </tr>
    }
</table>


